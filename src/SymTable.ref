*$ENUM SymTable

$ENTRY ST-Create {
  = (SymTable);
}

/*
  Внутренний формат
  t.SymTable ::= (SymTable t.Reference*)
  t.Reference ::= (t.SrcPos e.Name)
*/

/**
  <ST-AddFunctionCall t.SymTable t.SrcPos e.Name> == t.SymTable
*/
$ENTRY ST-AddFunctionCall {
  (SymTable e.References) t.SrcPos e.Name =
    (SymTable e.References (t.SrcPos e.Name));
}

/**
  <ST-GetAllReferences t.SymTable> == (t.SrcPos e.Name)*
*/
$ENTRY ST-GetAllReferences {
  (SymTable e.References) = e.References;
}
