$EXTERN WriteLine;

$ENUM Tag1, Tag2;

TestADT {
  = <DoTestADT [Tag1] 'abcd' 'efgh' [Tag2]>;
}

DoTestADT {
  [Tag1 e.1] s.Next e.Inner s.Last [Tag2 e.2] =
    <DoTestADT
      [Tag1 e.1 s.Next] e.Inner [Tag2 s.Last e.2]
    >;

  [Tag1 e.1] [Tag2 e.2] = <WriteLine 'ADT Success'>;
}

$ENUM TestDump;

$EXTERN SymbCompare;

$ENUM A;

TestOrder {
  = <WriteLine <SymbCompare 'I' A>>;
}

$ENTRY Go {
  =
    <TestADT>
    //<TestDump [Tag1 'x'] [Tag2 100]>
    <WriteLine 'Test print ' [Tag1 [Tag2 ]]>
    <TestOrder>;
}
